{
  "mongo": "env:MONGO",
  "express": {
    "env": "", // NOTE: `env` is managed by the framework. This value will be overwritten.
    "x-powered-by": false,
    "trust proxy": false,
    "jsonp callback name": null,
    "json replacer": null,
    "json spaces": 0,
    "case sensitive routing": false,
    "strict routing": false,
    "view cache": false,
    "view engine": "jade",
    "views": "path:./views",
    "route": "/"
  },
  "middleware": {
    "static": {
      "module": {
        "arguments": [
          "path:./.build"
        ]
      }
    },
    "appsec": {
      "enabled": false
    },
    "flash": {
      "enabled":  true,
      "priority": 111,
      "module": {
        "name": "connect-flash"
      }
    },
    "viewshelpers": {
      "enabled":  true,
      "priority": 112, 
      "module": {
        "name": "view-helpers"
      }
    },
    "router": {
      "module": {
        "arguments": [
          {
            "directory": "path:./controllers"
          }
        ]
      }
    }
  }
}
